{
  "hosting": {
    "public": "build",
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "ignore": ["firebase.json", "package.json", "node_modules/**", "src/**", ".git/**"]
  }
}
